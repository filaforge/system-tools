{
  "name": "filaforge/system-tools",
  "description": "System tools plugin",
  "type": "library",
  "license": "MIT",
  "require": {
  "php": ">=8.1",
  "filament/filament": "^4.0",
  "illuminate/support": "^12.0",
  "spatie/laravel-package-tools": "^1.15"
  },
  "autoload": {
    "psr-4": {
      "Filaforge\\SystemTools\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Filaforge\\SystemTools\\Providers\\SystemToolsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

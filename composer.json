{
  "name": "filaforge/system-tools",
  "description": "System tools plugin",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "filament/filament": "^4.0",
    "laravel/framework": "^12.0",
    "spatie/laravel-package-tools": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Filaforge\\SystemTools\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Filaforge\\SystemTools\\Providers\\SystemToolsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
